{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "strict": false,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
